msgid "Wavelets"
msgstr "Ondetes"

#: plugins/impex/raw/3rdparty/libkdcraw/src/dcrawsettingswidget.cpp:594
#, kde-format
msgctxt "@item:inlistbox Noise Reduction"
msgid "FBDD"
msgstr "FBDD"

